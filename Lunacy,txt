-- Entity
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local GameData = ReplicatedStorage:FindFirstChild("GameData")
local CurrentRooms = game.Workspace:FindFirstChild("CurrentRooms")
local LatestRoomm = CurrentRooms[game.ReplicatedStorage.GameData.LatestRoom.Value]

local IsMiddleFloor = game.ReplicatedStorage.GameData.LatestRoom.Value == 50
local IsGreenHouse = game.ReplicatedStorage.GameData.LatestRoom.Value > 90 and game.ReplicatedStorage.GameData.LatestRoom.Value < 100
local IsEletrical = game.ReplicatedStorage.GameData.LatestRoom.Value == 100 and GameData.Floor.Value == "Hotel"
local IsSeekChase = game.Workspace:FindFirstChild("SeekMovingNewClone") or game.Workspace:FindFirstChild("SeekMoving")
local IsFigure = LatestRoomm:FindFirstChild("FigureRig") or LatestRoomm:FindFirstChild("FigureSetup")
local IsHalt = LatestRoomm:GetAttribute("RawName") == "HaltHallway"
local IsGrumble = GameData.Floor.Value == "Mines" and IsMiddleFloor
local player = game.Players.LocalPlayer
local mainGame = require(player.PlayerGui.MainUI.Initiator.Main_Game)
local screechModule = require(player.PlayerGui.MainUI.Initiator.Main_Game.RemoteListener.Modules.Screech)

local code = 'https://raw.githubusercontent.com/PhamNgocHaMy2012/Script/main/'

local function screech()
if not IsSeekChase then
screechModule(mainGame)
end
end

local function Electricer()
if not IsSeekChase then
loadstring(game:HttpGet(code .. 'Electricer.txt'))()
end
end

local function flare()
game.ReplicatedStorage.GameData.LatestRoom.Changed:Wait()
if not IsMiddleFloor or IsGreenHouse or IsEletrical or IsSeekChase or IsFigure or IsHalt or IsGrumble then
loadstring(game:HttpGet(code .. 'flare.txt'))()
end
end

local function spec()
game.ReplicatedStorage.GameData.LatestRoom.Changed:Wait()
if not IsMiddleFloor or IsGreenHouse or IsEletrical or IsSeekChase or IsFigure or IsHalt or IsGrumble then
loadstring(game:HttpGet(code .. 'Spectrer.txt'))()
end
end

local function godly()
game.ReplicatedStorage.GameData.LatestRoom.Changed:Wait()
if not IsMiddleFloor or IsGreenHouse or IsEletrical or IsSeekChase or IsFigure or IsHalt or IsGrumble then
loadstring(game:HttpGet(code .. 'Godly.txt'))()
end
end

local function trun()
game.ReplicatedStorage.GameData.LatestRoom.Changed:Wait()
wait(math.random(5, 10))
if not IsMiddleFloor or IsGreenHouse or IsEletrical or IsSeekChase or IsFigure or IsHalt or IsGrumble then
loadstring(game:HttpGet(code .. 'trun.txt'))()
end
end

