coroutine.wrap(function() -- Window
local r = math.random(150, 255)
local b = math.random(150, 255)
local color = Color3.fromRGB(r, 0, b)
            
local CurrentRooms = workspace:WaitForChild("CurrentRooms")
local RunService = game:GetService("RunService")
local tweenservice = game:GetService("TweenService")

local function convertToDay(Room: Model): ()
    task.wait(0.5) -- Stream In
    local IndexedWindows = {}

    for _, RoomAsset in Room:GetDescendants() do
        if not RoomAsset:IsA("Model") then

            continue
        end

        if RoomAsset.Name == "Window" or RoomAsset.Name == "Window_Tall" then
            table.insert(IndexedWindows, RoomAsset)
        end

        RunService.Heartbeat:Wait()
    end

    for _, Window in IndexedWindows do
        local Success, Error = pcall(
            function(): ()
                local Skybox = Window.Skybox
                local LightPanel = Window.LightPanel
                local Glass = Window.Glass
                local SurfaceLight = LightPanel.SurfaceLight
                local NewSurfaceLight = SurfaceLight:Clone()

                for _, Child in Glass:GetChildren() do
                    if Child:IsA("Sound") then
                        Child.Volume = 0.5
                        Child.PlaybackSpeed = 1
                        Child.SoundId = "rbxassetid://9064263922"
                    end
                end

                for _, Child in LightPanel:GetChildren() do
                    Child:Destroy()
                end

                NewSurfaceLight.Parent = LightPanel
                NewSurfaceLight.Name = "Daylight"
                task.spawn(function()
                while true do wait(3)
                tweenservice:Create(NewSurfaceLight,TweenInfo.new(3,Enum.EasingStyle.Linear),{Color = color}):Play()
                tweenservice:Create(NewSurfaceLight,TweenInfo.new(3,Enum.EasingStyle.Linear),{Brightness = 0.5}):Play()
                tweenservice:Create(NewSurfaceLight,TweenInfo.new(3,Enum.EasingStyle.Linear),{Range = math.random(20, 30)}):Play()
                end
                end)
                NewSurfaceLight.Angle = 120
                NewSurfaceLight.Shadows = true
                NewSurfaceLight.Enabled = true

                Skybox.Color = color
                local Forcer = Skybox:GetPropertyChangedSignal("Color"):Connect(
                    function(): ()
                        Skybox.Color = color
                    end
                )

                Room.Destroying:Once(
                    function(): ()
                        Forcer:Disconnect()
                    end
                )
            end
        )

        assert(Success, Error)
    end
end

CurrentRooms.ChildAdded:Connect(
    function(Room: Model): ()
        task.spawn(convertToDay, Room)
    end
)

for _, Room in CurrentRooms:GetChildren() do
    task.spawn(convertToDay, Room)
end
end)()

coroutine.wrap(function() -- Light
local LightReplaceModel = game:GetObjects("rbxassetid://118591319278236")[1]

local function light()
for i,v in pairs(workspace.CurrentRooms[game:GetService("ReplicatedStorage").GameData.LatestRoom.Value].Assets.Light_Fixtures:GetDescendants()) do
	if v.Name == "LightStand" or v.Name == "LightStandSingle" then
			local torch = LightReplaceModel:Clone()
 			torch.Parent = workspace.CurrentRooms[game:GetService("ReplicatedStorage").GameData.LatestRoom.Value].Assets.Light_Fixtures
		 	torch:PivotTo(v:GetPivot())
	end
end
end
light()

game.ReplicatedStorage.GameData.LatestRoom.Changed:Connect(function()
light()
end)

end)()

