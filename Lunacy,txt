local painters = 1
local paints = {
	[1] = {"rbxassetid://12625633899","Depth The Dark."},
	[2] = {"rbxassetid://11395249132","Odd feline Specimen."},
	[3] = {"rbxassetid://16585037950","The Angel..."},
	[4] = {"rbxassetid://13377332330","Demonic..."},
	[5] = {"rbxassetid://11246446505","Nightmare."},
